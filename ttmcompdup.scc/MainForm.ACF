Private Sub Command17_Click()

Dim db As DAO.Database
Dim rs As DAO.Recordset

Set db = CurrentDb

Dim cond As String
cond = "false "
Set rs = db.OpenRecordset("SELECT EventDateID, RunningOrderID " & _
    "FROM qryCurrentEventROEntries")
    
Do While Not rs.EOF
    cond = cond & " OR( EventDateID = " & rs!EventDateID & " AND RunningOrderID = " & rs!RunningOrderID & ") "
    rs.MoveNext
Loop
        
Debug.Print cond
Dim stDocName As String
Dim where As String

stDocName = "rptRunningOrderSmall"
     
DoCmd.OpenReport stDocName, acPreview, , cond
End Sub

