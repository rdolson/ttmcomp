Private Sub saveStuffInTable(db As DAO.Database)
db.Execute ("delete from ltblSelectedDate")

With Me.trial_date
    For Each varItem In .ItemsSelected
        If Not IsNull(varItem) Then
            'Build up the filter from the bound column (hidden).
            db.Execute ("INSERT INTO ltblSelectedDate VALUES ('" & .ItemData(varItem) & "')")
            
        End If
    Next
End With

db.Execute ("delete from ltblSelectedRO")

With Me.ro
    For Each varItem In .ItemsSelected
        If Not IsNull(varItem) Then
            db.Execute ("INSERT INTO ltblSelectedRO VALUES ('" & .ItemData(varItem) & "')")
        End If
    Next
End With
End Sub

Private Sub gate_for_selected_ro_Click()
Dim db As DAO.Database
Dim rs As DAO.Recordset

Set db = CurrentDb

Dim cond As String

cond = "(RunningOrderID = " & Me!ro & ")"

Call saveStuffInTable(db)


Dim rocond As String
rocond = "false"
With Me.ro
    For Each varItem In .ItemsSelected
        If Not IsNull(varItem) Then
            'Build up the filter from the bound column (hidden).
            rocond = rocond & " OR RunningOrderID = " & .ItemData(varItem)
        End If
    Next
End With
cond = rocond
Debug.Print cond
Dim stDocName As String
Dim where As String

stDocName = "rptRunningOrderAllDays"
     
DoCmd.OpenReport stDocName, acPreview, , cond
End Sub

